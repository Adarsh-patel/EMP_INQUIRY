<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <template id="emp_form">
            <t t-call="website.layout">
                <div class="container">
                    <div class="panel reg_form_panel">
                        <div class="panel-header text-center">
                            <h1>
                                <i class="fa fa-user-plus" aria-hidden="true"></i>
                                <strong>Inquiry Form</strong>
                            </h1>
                        </div>
                        <div class="panel-body text-center">
                            <form class="form-style col-md-12">
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Customer No:-</label>
                                    <div class="col-sm-6">
                                        <input name="customer_no" type="int" class="form-control" placeholder="Number..." required="True"/>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Name:-</label>
                                    <div class="col-sm-3">
                                        <input name="first_name" class="form-control align-left" placeholder="First Name" required="True"/>
                                    </div>
                                    <div class="col-sm-3">
                                        <input name="last_name" class="form-control align-right" placeholder="Last Name" required="True"/>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Address:-</label>
                                    <div class="col-sm-6">
                                        <input name="street" class="form-control" placeholder="Street" required="True"/>
                                    </div><br/>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-sm-2"></label>
                                    <div class="col-sm-3">
                                        <input name="city" class="form-control align-left" placeholder="City" required="True"/>
                                    </div>
                                    <div class="col-sm-3">
                                        <input name="country" class="form-control align-right" placeholder="Country" required="True"/>
                                    </div>
                                </div><br/><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Date Of Birth:-</label>
                                    <div class="col-sm-6">
                                        <input name="dob" type="date" class="form-control" placeholder="Birth Date" required="True"/>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Proof ID:-</label>
                                    <div class="col-sm-6">
                                        <input name="proof_id" type="text" class="form-control" placeholder="Id" required="True"/>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Security No:-</label>
                                    <div class="col-sm-6">
                                        <input name="security_no" min="9" max="9"
                                               type="text" class="form-control" placeholder="Security no" required="True"/>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Employer Name:-</label>
                                    <div class="col-sm-6">
                                        <input name="employer_name" type="text" class="form-control" placeholder="Employer Name" required="True"/>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Employer Income:-</label>
                                    <div class="col-sm-6">
                                        <input name="employer_income" type="text" class="form-control" placeholder="Employer Income" required="True"/>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2"></label>
                                    <div class="col-sm-3">
                                        <h4><strong>Bank Details</strong></h4>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Account No:-</label>
                                    <div class="col-sm-6">
                                        <input name="account_no" type="text" class="form-control" placeholder="Account No" required="True"/>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">IFSC Code:-</label>
                                    <div class="col-sm-6">
                                        <input name="ifsc_code" type="text" class="form-control" placeholder="IFSC Code" required="True"/>
                                    </div>
                                </div><br/><br/>
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Notes:-</label>
                                    <div class="col-sm-6">
                                        <input name="notes" type="text" class="form-control" placeholder="Notes" required="True"/>
                                    </div>
                                </div><br/><br/>


                                <div class="form-group">
                                    <div class="col-sm-offset-2 col-sm-4 ">
                                        <button type="submit" class="btn btn-primary" value="submit">Submit</button>
                                        <button type="reset" class="btn btn-default" value="reset">Cancel</button>
                                    </div>
                                </div>






                            </form>
